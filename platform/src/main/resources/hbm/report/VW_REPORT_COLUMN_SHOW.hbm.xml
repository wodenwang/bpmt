<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class entity-name="VwReportColumnShow" table="VW_REPORT_COLUMN_SHOW">
    <cache usage="read-write" />
    <comment>动态表展示列视图模块</comment>

    <id name="id" type="java.lang.Long">
      <column name="ID" length="12" not-null="true">
        <comment>自动主键</comment>
      </column>
      <generator class="increment"></generator>
    </id>

    <property name="viewKey" type="java.lang.String">
      <column name="VIEW_KEY" length="100" not-null="true">
        <comment>主键KEY</comment>
      </column>
    </property>

    <property name="busiName" type="java.lang.String">
      <column name="BUSI_NAME" length="200" not-null="true">
        <comment>展示名</comment>
      </column>
    </property>

    <property name="style" type="java.lang.String">
      <column name="STYLE" length="500">
        <comment>列样式</comment>
      </column>
    </property>

    <property name="whole" type="java.lang.Integer">
      <column name="WHOLE" length="8" not-null="true">
        <comment>占据全行</comment>
      </column>
    </property>

    <property name="sortField" type="java.lang.String">
      <column name="SORT_FIELD" length="100">
        <comment>排序字段</comment>
      </column>
    </property>

    <property name="contentType" type="java.lang.Integer">
      <column name="CONTENT_TYPE" length="8" not-null="true">
        <comment>内容脚本类型</comment>
      </column>
    </property>

    <property name="contentScript" type="text">
      <column name="CONTENT_SCRIPT" not-null="true">
        <comment>内容脚本</comment>
      </column>
    </property>

    <property name="summaryContentType" type="java.lang.Integer">
      <column name="SUMMARY_CONTENT_TYPE" length="8">
        <comment>汇集内容脚本类型</comment>
      </column>
    </property>

    <property name="summaryContentScript" type="text">
      <column name="SUMMARY_CONTENT_SCRIPT">
        <comment>汇集内容脚本</comment>
      </column>
    </property>

    <property name="sort" type="java.lang.Integer">
      <column name="SORT" length="8" not-null="true">
        <comment>排序</comment>
      </column>
    </property>

    <property name="listSort" type="java.lang.Integer">
      <column name="LIST_SORT" length="8" not-null="true">
        <comment>列表页排序</comment>
      </column>
    </property>

    <many-to-one name="pri" class="com.riversoft.platform.po.CmPri" cascade="all" lazy="false">
      <column name="PRI" length="100" not-null="true">
        <comment>权限资源</comment>
      </column>
    </many-to-one>

  </class>
</hibernate-mapping>
