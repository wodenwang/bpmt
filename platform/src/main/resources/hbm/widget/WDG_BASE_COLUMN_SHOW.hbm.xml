<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="WdgBaseColumnShow" table="WDG_BASE_COLUMN_SHOW">
		<cache usage="read-write" />
		<comment>展示列</comment>

		<id name="id" type="java.lang.Long">
			<column name="ID" length="12" not-null="true">
				<comment>自动主键</comment>
			</column>
			<generator class="increment"></generator>
		</id>

		<property name="inWait" type="java.lang.Integer">
			<column name="IN_WAIT" length="8" not-null="true">
				<comment>待选页</comment>
			</column>
		</property>

		<property name="inSelected" type="java.lang.Integer">
			<column name="IN_SELECTED" length="8" not-null="true">
				<comment>已选页</comment>
			</column>
		</property>

		<property name="inResult" type="java.lang.Integer">
			<column name="IN_RESULT" length="8" not-null="true">
				<comment>结果页</comment>
			</column>
		</property>

		<property name="widgetKey" type="java.lang.String">
			<column name="WIDGET_KEY" length="100" not-null="true">
				<comment>主键KEY</comment>
			</column>
		</property>

		<property name="busiName" type="java.lang.String">
			<column name="BUSI_NAME" length="200" not-null="true">
				<comment>展示名</comment>
			</column>
		</property>

		<property name="style" type="java.lang.String">
			<column name="STYLE" length="500">
				<comment>列样式</comment>
			</column>
		</property>

		<property name="contentType" type="java.lang.Integer">
			<column name="CONTENT_TYPE" length="8" not-null="true">
				<comment>内容脚本类型</comment>
			</column>
		</property>

		<property name="contentScript" type="text">
			<column name="CONTENT_SCRIPT" not-null="true">
				<comment>内容脚本</comment>
			</column>
		</property>

		<property name="sortField" type="java.lang.String">
			<column name="SORT_FIELD" length="200">
				<comment>排序字段</comment>
			</column>
		</property>

		<property name="sort" type="java.lang.Integer">
			<column name="SORT" length="8" not-null="true">
				<comment>排序</comment>
			</column>
		</property>

		<many-to-one name="pri" class="com.riversoft.platform.po.CmPri" cascade="all" lazy="false">
			<column name="PRI" length="100" not-null="true">
				<comment>权限资源</comment>
			</column>
		</many-to-one>

	</class>
</hibernate-mapping>
