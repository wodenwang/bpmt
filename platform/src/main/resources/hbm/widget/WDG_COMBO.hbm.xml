<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="WdgCombo" table="WDG_COMBO">
		<cache usage="read-write" />
		<comment>高级选择控件</comment>

		<id name="widgetKey" type="java.lang.String">
			<column name="WIDGET_KEY" length="100" not-null="true">
				<comment>主键KEY</comment>
			</column>
		</id>

		<property name="codeType" type="java.lang.Integer">
			<column name="CODE_TYPE" length="8" not-null="true">
				<comment>CODE逻辑(脚本类型)</comment>
			</column>
		</property>

		<property name="codeScript" type="text">
			<column name="CODE_SCRIPT" not-null="true">
				<comment>CODE逻辑(脚本)</comment>
			</column>
		</property>

		<property name="nameType" type="java.lang.Integer">
			<column name="NAME_TYPE" length="8" not-null="true">
				<comment>NAME逻辑(脚本类型)</comment>
			</column>
		</property>

		<property name="nameScript" type="text">
			<column name="NAME_SCRIPT" not-null="true">
				<comment>NAME逻辑(脚本)</comment>
			</column>
		</property>

		<property name="pkSqlType" type="java.lang.Integer">
			<column name="PK_SQL_TYPE" length="8">
				<comment>唯一值SQL片段(脚本类型)</comment>
			</column>
		</property>

		<property name="pkSqlScript" type="text">
			<column name="PK_SQL_SCRIPT">
				<comment>唯一值SQL片段(脚本)</comment>
			</column>
		</property>

	</class>
</hibernate-mapping>