<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class entity-name="WdgBase" table="WDG_BASE">
    <cache usage="read-write" />
    <comment>数据控件</comment>

    <id name="widgetKey" type="java.lang.String">
      <column name="WIDGET_KEY" length="100" not-null="true">
        <comment>主键KEY</comment>
      </column>
    </id>

    <property name="busiName" type="java.lang.String">
      <column name="BUSI_NAME" length="200" not-null="true">
        <comment>展示名</comment>
      </column>
    </property>

    <!--js脚本 -->
    <property name="jsType" type="java.lang.Integer">
      <column name="JS_TYPE" length="8">
        <comment>客户端脚本类型</comment>
      </column>
    </property>
    <property name="jsScript" type="text">
      <column name="JS_SCRIPT">
        <comment>客户端脚本</comment>
      </column>
    </property>

    <property name="mainSqlType" type="java.lang.Integer">
      <column name="MAIN_SQL_TYPE" length="8" not-null="true">
        <comment>主SQL语句(脚本类型)</comment>
      </column>
    </property>
    <property name="mainSqlScript" type="text">
      <column name="MAIN_SQL_SCRIPT" not-null="true">
        <comment>主SQL语句</comment>
      </column>
    </property>

    <property name="orderBy" type="java.lang.String">
      <column name="ORDER_BY" length="200">
        <comment>排序语句</comment>
      </column>
    </property>

    <property name="width" type="java.lang.Integer">
      <column name="WIDTH" length="8" not-null="false">
        <comment>弹出框宽度</comment>
      </column>
    </property>

    <property name="pageLimit" type="java.lang.Integer">
      <column name="PAGE_LIMIT" length="8" not-null="false">
        <comment>每页数量</comment>
      </column>
    </property>

    <property name="initQuery" type="java.lang.Integer">
      <column name="INIT_QUERY" length="8" not-null="true">
        <comment>初始化查询</comment>
      </column>
    </property>

    <property name="createUid" type="java.lang.String">
      <column name="CREATE_UID" length="100" not-null="true">
        <comment>创建人</comment>
      </column>
    </property>

    <property name="updateDate" type="java.util.Date">
      <column name="UPDATE_DATE" length="23" not-null="true">
        <comment>更新时间</comment>
      </column>
    </property>

    <property name="createDate" type="java.util.Date">
      <column name="CREATE_DATE" length="23" not-null="true">
        <comment>创建时间</comment>
      </column>
    </property>

    <property name="description" type="java.lang.String">
      <column name="DESCRIPTION" length="500">
        <comment>描述</comment>
      </column>
    </property>

    <set name="querys" inverse="true" cascade="delete" lazy="false" order-by="sort asc">
      <cache usage="read-write" />
      <comment>查询条件列表</comment>
      <key>
        <column name="WIDGET_KEY" />
      </key>
      <one-to-many entity-name="WdgBaseQuery" />
    </set>

    <set name="showColumns" inverse="true" cascade="delete" lazy="false" order-by="sort asc">
      <cache usage="read-write" />
      <comment>展示类字段列表</comment>
      <key>
        <column name="WIDGET_KEY" />
      </key>
      <one-to-many entity-name="WdgBaseColumnShow" />
    </set>

    <set name="formColumns" inverse="true" cascade="delete" lazy="false" order-by="sort asc">
      <cache usage="read-write" />
      <comment>表单字段列表</comment>
      <key>
        <column name="WIDGET_KEY" />
      </key>
      <one-to-many entity-name="WdgBaseColumnForm" />
    </set>

    <set name="limits" inverse="true" cascade="delete" lazy="false" order-by="sort asc">
      <cache usage="read-write" />
      <comment>数据约束列表</comment>
      <key>
        <column name="WIDGET_KEY" />
      </key>
      <one-to-many entity-name="WdgBaseLimit" />
    </set>

  </class>
</hibernate-mapping>
