<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class entity-name="WfUserTask" table="WF_USER_TASK">
    <cache usage="read-write" />
    <comment>用户任务配置</comment>

    <composite-id>
      <key-property name="pdId" type="java.lang.String">
        <column name="PD_ID" length="100" not-null="true">
          <comment>流程定义ID</comment>
        </column>
      </key-property>
      <key-property name="activityId" type="java.lang.String">
        <column name="ACTIVITY_ID" length="100" not-null="true">
          <comment>节点ID</comment>
        </column>
      </key-property>
    </composite-id>

    <!--js脚本 -->
    <property name="jsType" type="java.lang.Integer">
      <column name="JS_TYPE" length="8">
        <comment>客户端脚本类型</comment>
      </column>
    </property>
    <property name="jsScript" type="text">
      <column name="JS_SCRIPT">
        <comment>客户端脚本</comment>
      </column>
    </property>

    <!-- 通知触发时机 -->
    <property name="notifyType" type="java.lang.String">
      <column name="NOTIFY_TYPE" length="100">
        <comment>通知触发类型(多选,1:任务接收人;2:转发接收人;3:群组任务触发)</comment>
      </column>
    </property>

    <property name="description" type="java.lang.String">
      <column name="DESCRIPTION" length="500">
        <comment>描述</comment>
      </column>
    </property>

  </class>

  <!-- 人员分配 -->
  <class entity-name="WfUserTaskAssignee" table="WF_USER_TASK_ASSIGNEE">
    <cache usage="read-write" />
    <comment>用户节点人员分配规则</comment>

    <id name="id" type="java.lang.Long">
      <column name="ID" length="12" not-null="true">
        <comment>自动主键</comment>
      </column>
      <generator class="increment"></generator>
    </id>

    <property name="pdId" type="java.lang.String">
      <column name="PD_ID" length="100" not-null="true">
        <comment>流程定义ID</comment>
      </column>
    </property>

    <property name="activityId" type="java.lang.String">
      <column name="ACTIVITY_ID" length="100" not-null="true">
        <comment>流程定义ID</comment>
      </column>
    </property>

    <property name="decideType" type="java.lang.Integer">
      <column name="DECIDE_TYPE" length="8" not-null="true">
        <comment>成立条件-脚本类型</comment>
      </column>
    </property>

    <property name="decideScript" type="text">
      <column name="DECIDE_SCRIPT" not-null="true">
        <comment>成立条件-脚本</comment>
      </column>
    </property>

    <!-- 0:用户,1:组织,2:角色 ,3:组织+角色 -->
    <property name="allocateType" type="java.lang.Integer">
      <column name="ALLOCATE_TYPE" length="8" not-null="true">
        <comment>分配方式</comment>
      </column>
    </property>

    <!-- 独占字段 -->
    <property name="uniqueFlag" type="java.lang.Integer">
      <column name="UNIQUE_FLAG" length="8" not-null="true" default="0">
        <comment>是否独占</comment>
      </column>
    </property>

    <!-- 分批判断 -->
    <property name="batchNum" type="java.lang.Integer">
      <column name="BATCH_NUM" length="8" not-null="true">
        <comment>批次</comment>
      </column>
    </property>

    <property name="uidType" type="java.lang.Integer">
      <column name="UID_TYPE" length="8">
        <comment>分配用户-脚本类型</comment>
      </column>
    </property>

    <property name="uidScript" type="text">
      <column name="UID_SCRIPT">
        <comment>分配用户-脚本</comment>
      </column>
    </property>

    <property name="roleType" type="java.lang.Integer">
      <column name="ROLE_TYPE" length="8">
        <comment>分配角色-脚本类型</comment>
      </column>
    </property>

    <property name="roleScript" type="text">
      <column name="ROLE_SCRIPT">
        <comment>分配角色-脚本</comment>
      </column>
    </property>

    <property name="groupType" type="java.lang.Integer">
      <column name="GROUP_TYPE" length="8">
        <comment>分配组织-脚本类型</comment>
      </column>
    </property>

    <property name="groupScript" type="text">
      <column name="GROUP_SCRIPT">
        <comment>分配组织-脚本</comment>
      </column>
    </property>

    <property name="sort" type="java.lang.Integer">
      <column name="SORT" length="8" not-null="true">
        <comment>排序</comment>
      </column>
    </property>

    <property name="description" type="java.lang.String">
      <column name="DESCRIPTION" length="500">
        <comment>描述</comment>
      </column>
    </property>
