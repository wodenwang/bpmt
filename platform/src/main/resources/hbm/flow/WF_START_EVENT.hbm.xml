<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class entity-name="WfStartEvent" table="WF_START_EVENT">
    <cache usage="read-write" />
    <comment>开始节点配置</comment>

    <id name="pdId" type="java.lang.String">
      <column name="PD_ID" length="100" not-null="true">
        <comment>流程定义ID</comment>
      </column>
    </id>

    <!--js脚本 -->
    <property name="jsType" type="java.lang.Integer">
      <column name="JS_TYPE" length="8">
        <comment>客户端脚本类型</comment>
      </column>
    </property>
    <property name="jsScript" type="text">
      <column name="JS_SCRIPT">
        <comment>客户端脚本</comment>
      </column>
    </property>

    <property name="description" type="java.lang.String">
      <column name="DESCRIPTION" length="500">
        <comment>描述</comment>
      </column>
    </property>
  </class>

  <!-- 保存 -->
  <class entity-name="WfStartEventBtnSave" table="WF_START_EVENT_BTN_SAVE">
    <cache usage="read-write" />
    <comment>节点按钮-保存</comment>
    <composite-id>
      <key-property name="pdId" type="java.lang.String">
        <column name="PD_ID" length="100" not-null="true">
          <comment>流程定义ID</comment>
        </column>
      </key-property>
    </composite-id>

    <property name="busiName" type="java.lang.String">
      <column name="BUSI_NAME" length="200" not-null="true">
        <comment>展示名</comment>
      </column>
    </property>

    <property name="icon" type="java.lang.String">
      <column name="ICON" length="100" not-null="true">
        <comment>图标</comment>
      </column>
    </property>

    <property name="styleClass" type="java.lang.String">
      <column name="STYLE_CLASS" length="100">
        <comment>对齐方式(left/right/center)</comment>
      </column>
    </property>

    <property name="sort" type="java.lang.Integer">
      <column name="SORT" length="8" not-null="true">
        <comment>排序</comment>
      </column>
    </property>

    <property name="description" type="java.lang.String">
      <column name="DESCRIPTION" length="500">
        <comment>描述</comment>
      </column>
    </property>

    <!-- 是否允许转发 -->
    <property name="checkType" type="java.lang.Integer">
      <column name="CHECK_TYPE" length="8" not-null="true">
        <comment>展示条件(脚本类型)</comment>
      </column>
    </property>
    <property name="checkScript" type="text">
      <column name="CHECK_SCRIPT" not-null="true">
        <comment>展示条件(脚本)</comment>
      </column>
    </property>

    <property name="loading" type="java.lang.Integer">
      <column name="LOADING" length="8" not-null="true">
        <comment>高级进度条</comment>
      </column>
    </property>
  </class>

  <!-- 启动 -->
  <class entity-name="WfStartEventBtnStart" table="WF_START_EVENT_BTN_START">
    <cache usage="read-write" />
    <comment>节点按钮-启动</comment>
