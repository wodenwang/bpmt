    <property name="description" type="java.lang.String">
      <column name="DESCRIPTION" length="500">
        <comment>描述</comment>
      </column>
    </property>

    <!-- confirm确认信息 -->
    <property name="confirmType" type="java.lang.Integer">
      <column name="CONFIRM_TYPE" length="8">
        <comment>确认信息(脚本类型)</comment>
      </column>
    </property>
    <property name="confirmScript" type="text">
      <column name="CONFIRM_SCRIPT" length="1000">
        <comment>确认信息(脚本)</comment>
      </column>
    </property>

    <property name="opinionFlag" type="java.lang.Integer">
      <column name="OPINION_FALG" length="8" not-null="true" default="0">
        <comment>是否使用审批意见</comment>
      </column>
    </property>

    <!-- 是否允许转发 -->
    <property name="checkType" type="java.lang.Integer">
      <column name="CHECK_TYPE" length="8" not-null="true">
        <comment>展示条件(脚本类型)</comment>
      </column>
    </property>
    <property name="checkScript" type="text">
      <column name="CHECK_SCRIPT" not-null="true">
        <comment>展示条件(脚本)</comment>
      </column>
    </property>

    <property name="quickOpinionType" type="java.lang.Integer">
      <column name="QUICK_OPINION_TYPE" length="8">
        <comment>快速回复(脚本类型)</comment>
      </column>
    </property>

    <property name="quickOpinionScript" type="text">
      <column name="QUICK_OPINION_SCRIPT" length="1000">
        <comment>快速回复(脚本)</comment>
      </column>
    </property>

    <property name="loading" type="java.lang.Integer">
      <column name="LOADING" length="8" not-null="true">
        <comment>高级进度条</comment>
      </column>
    </property>
  </class>

  <!-- 继承字段(继承自基础视图) -->
