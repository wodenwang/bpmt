<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class entity-name="WfInclusiveGateway" table="WF_INCLUSIVE_GATEWAY">
    <cache usage="read-write" />
    <comment>包容网关节点配置</comment>

    <composite-id>
      <key-property name="pdId" type="java.lang.String">
        <column name="PD_ID" length="100" not-null="true">
          <comment>流程定义ID</comment>
        </column>
      </key-property>
      <key-property name="activityId" type="java.lang.String">
        <column name="ACTIVITY_ID" length="100" not-null="true">
          <comment>节点ID</comment>
        </column>
      </key-property>
    </composite-id>

    <property name="description" type="java.lang.String">
      <column name="DESCRIPTION" length="500">
        <comment>描述</comment>
      </column>
    </property>

  </class>

  <!-- 判断条件 -->
  <class entity-name="WfInclusiveGatewayDecide" table="WF_INCLUSIVE_GATEWAY_DECIDE">
    <cache usage="read-write" />
    <comment>判断条件</comment>

    <id name="id" type="java.lang.Long">
      <column name="ID" length="12" not-null="true">
        <comment>自动主键</comment>
      </column>
      <generator class="increment"></generator>
    </id>

    <property name="pdId" type="java.lang.String">
      <column name="PD_ID" length="100" not-null="true">
        <comment>流程定义ID</comment>
      </column>
    </property>

    <property name="activityId" type="java.lang.String">
      <column name="ACTIVITY_ID" length="100" not-null="true">
        <comment>流程定义ID</comment>
      </column>
    </property>

    <property name="flowId" type="java.lang.String">
      <column name="FLOW_ID" length="100" not-null="true">
        <comment>连线ID</comment>
      </column>
    </property>

    <property name="decideType" type="java.lang.Integer">
      <column name="DECIDE_TYPE" length="8" not-null="true">
        <comment>脚本类型</comment>
      </column>
    </property>

    <property name="decideScript" type="text">
      <column name="DECIDE_SCRIPT"  not-null="true">
        <comment>脚本</comment>
      </column>
    </property>

    <property name="sort" type="java.lang.Integer">
      <column name="SORT" length="8" not-null="true">
        <comment>排序</comment>
      </column>
    </property>

    <property name="description" type="java.lang.String">
      <column name="DESCRIPTION" length="500">
        <comment>描述</comment>
      </column>
    </property>

  </class>

</hibernate-mapping>
