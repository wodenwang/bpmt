<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class entity-name="UsRolePriGroupRelate" table="US_ROLE_PRI_GROUP_RELATE">
    <cache usage="read-write" />
    <comment>角色权限组关联</comment>
    <composite-id>
      <key-property name="roleKey" type="java.lang.String">
        <column name="ROLE_KEY" length="100" not-null="true">
          <comment>角色KEY</comment>
        </column>
      </key-property>
      <key-property name="groupId" type="java.lang.String">
        <column name="GROUP_ID" length="100" not-null="true">
          <comment>权限组ID</comment>
        </column>
      </key-property>
    </composite-id>
    <property name="sysFlag" type="java.lang.Integer">
      <column name="SYS_FLAG" length="8" not-null="true" default="0">
        <comment>是否系统级别</comment>
      </column>
    </property>
  </class>
</hibernate-mapping>
