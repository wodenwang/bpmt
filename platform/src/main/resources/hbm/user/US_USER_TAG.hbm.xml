<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class entity-name="UsUserTag" table="US_USER_TAG">
    <cache usage="read-write" />
    <comment>用户-标签关系表</comment>
    <composite-id>
      <key-property name="uid" type="java.lang.String">
        <column name="USER_ID" length="100" not-null="true">
          <comment>用户主键</comment>
        </column>
      </key-property>
      <key-property name="tagKey" type="java.lang.String">
        <column name="TAG_KEY" length="100" not-null="true">
          <comment>标签主键</comment>
        </column>
      </key-property>
    </composite-id>

    <property name="createDate" type="java.util.Date">
      <column name="CREATE_DATE" length="23" not-null="true">
        <comment>创建时间</comment>
      </column>
    </property>
    <property name="updateDate" type="java.util.Date">
      <column name="UPDATE_DATE" length="23" not-null="true">
        <comment>更新时间</comment>
      </column>
    </property>

  </class>
</hibernate-mapping>