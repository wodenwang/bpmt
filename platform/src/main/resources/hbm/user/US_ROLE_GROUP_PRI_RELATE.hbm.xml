<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class entity-name="UsRoleGroupPriRelate" table="US_ROLE_GROUP_PRI_RELATE">
    <cache usage="read-write" />
    <comment>角色组织权限组关联</comment>
    <composite-id>
      <key-property name="roleKey" type="java.lang.String">
        <column name="ROLE_KEY" length="100" not-null="true">
          <comment>角色KEY</comment>
        </column>
      </key-property>
      <key-property name="groupKey" type="java.lang.String">
        <column name="GROUP_KEY" length="100" not-null="true">
          <comment>组ID</comment>
        </column>
      </key-property>
      <key-property name="groupId" type="java.lang.String">
        <column name="GROUP_ID" length="100" not-null="true">
          <comment>权限组ID</comment>
        </column>
      </key-property>
    </composite-id>
  </class>
</hibernate-mapping>
