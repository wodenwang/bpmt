<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="VwDynColumn" table="VW_DYN_COLUMN">
		<cache usage="read-write" />
		<comment>动态表列视图模块</comment>

		<id name="id" type="java.lang.Long">
			<column name="ID" length="12" not-null="true">
				<comment>自动主键</comment>
			</column>
			<generator class="increment"></generator>
		</id>

		<property name="viewKey" type="java.lang.String" unique-key="VW_DYN_UNIQUE_KEY">
			<column name="VIEW_KEY" length="100" not-null="true">
				<comment>主键KEY</comment>
			</column>
		</property>

		<property name="name" type="java.lang.String" unique-key="VW_DYN_UNIQUE_KEY">
			<column name="NAME" length="100" not-null="true">
				<comment>字段名</comment>
			</column>
		</property>

		<property name="busiName" type="java.lang.String">
			<column name="BUSI_NAME" length="200" not-null="true">
				<comment>展示名</comment>
			</column>
		</property>

		<property name="tipType" type="java.lang.Integer">
			<column name="TIP_TYPE" length="8">
				<comment>提示信息(脚本类型)</comment>
			</column>
		</property>

		<property name="tipScript" type="text">
			<column name="TIP_SCRIPT">
				<comment>提示信息(脚本)</comment>
			</column>
		</property>

		<property name="showFlag" type="java.lang.Integer">
			<column name="SHOW_FLAG" length="8" not-null="true">
				<comment>是否展示</comment>
			</column>
		</property>
		
		<property name="formFlag" type="java.lang.Integer">
			<column name="FORM_FLAG" length="8" not-null="true">
				<comment>是否表单</comment>
			</column>
		</property>

		<property name="contentType" type="java.lang.Integer">
			<column name="CONTENT_TYPE" length="8" not-null="true">
				<comment>内容脚本类型</comment>
			</column>
		</property>

		<property name="contentScript" type="text">
			<column name="CONTENT_SCRIPT" not-null="true">
				<comment>内容脚本</comment>
			</column>
		</property>

		<property name="style" type="java.lang.String">
			<column name="STYLE" length="500">
				<comment>列样式</comment>
			</column>
		</property>

		<property name="whole" type="java.lang.Integer">
			<column name="WHOLE" length="8" not-null="true">
				<comment>占据全行</comment>
			</column>
		</property>

		<property name="widget" type="java.lang.String">
			<column name="WIDGET" length="200" not-null="true">
				<comment>绑定组件</comment>
			</column>
		</property>

		<property name="widgetParamType" type="java.lang.Integer">
			<column name="WIDGET_PARAM_TYPE" length="8">
				<comment>控件动态参数</comment>
			</column>
		</property>

		<property name="widgetParamScript" type="text">
			<column name="WIDGET_PARAM_SCRIPT">
				<comment>控件动态参数</comment>
			</column>
		</property>

		<property name="widgetContentType" type="java.lang.Integer">
			<column name="WIDGET_CONTENT_TYPE" length="8">
				<comment>表单值(脚本类型)</comment>
			</column>
		</property>

		<property name="widgetContentScript" type="text">
			<column name="WIDGET_CONTENT_SCRIPT">
				<comment>表单值(脚本)</comment>
			</column>
		</property>

		<property name="listSort" type="java.lang.Integer">
			<column name="LIST_SORT" length="8" not-null="true">
				<comment>列表页排序</comment>
			</column>
		</property>

		<property name="sort" type="java.lang.Integer">
			<column name="SORT" length="8" not-null="true">
				<comment>排序</comment>
			</column>
		</property>

		<property name="execType" type="java.lang.Integer">
			<column name="EXEC_TYPE" length="8">
				<comment>前置处理脚本类型</comment>
			</column>
		</property>

		<property name="execScript" type="text">
			<column name="EXEC_SCRIPT">
				<comment>前置处理脚本</comment>
			</column>
		</property>

		<many-to-one name="pri" class="com.riversoft.platform.po.CmPri"
			cascade="all" lazy="false">
			<column name="PRI" length="100" not-null="true">
				<comment>展示权限</comment>
			</column>
		</many-to-one>

		<many-to-one name="createPri" class="com.riversoft.platform.po.CmPri"
			cascade="all" lazy="false">
			<column name="CREATE_PRI" length="100">
				<comment>新增权限</comment>
			</column>
		</many-to-one>

		<many-to-one name="updatePri" class="com.riversoft.platform.po.CmPri"
			cascade="all" lazy="false">
			<column name="UPDATE_PRI" length="100">
				<comment>修改权限</comment>
			</column>
		</many-to-one>

	</class>
</hibernate-mapping>
