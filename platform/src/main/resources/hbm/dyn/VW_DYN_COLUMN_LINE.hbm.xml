<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="VwDynColumnLine" table="VW_DYN_COLUMN_LINE">
		<cache usage="read-write" />
		<comment>字段分割线</comment>

		<id name="id" type="java.lang.Long">
			<column name="ID" length="12" not-null="true">
				<comment>自动主键</comment>
			</column>
			<generator class="increment"></generator>
		</id>

		<property name="viewKey" type="java.lang.String">
			<column name="VIEW_KEY" length="100" not-null="true">
				<comment>视图主键</comment>
			</column>
		</property>

		<property name="busiName" type="java.lang.String">
			<column name="BUSI_NAME" length="200" not-null="true">
				<comment>展示名</comment>
			</column>
		</property>

		<property name="tipType" type="java.lang.Integer">
			<column name="TIP_TYPE" length="8">
				<comment>提示信息(脚本类型)</comment>
			</column>
		</property>

		<property name="tipScript" type="text">
			<column name="TIP_SCRIPT">
				<comment>提示信息(脚本)</comment>
			</column>
		</property>

		<property name="expandFlag" type="java.lang.Integer">
			<column name="EXPAND_FLAG" length="8" not-null="true">
				<comment>是否展开</comment>
			</column>
		</property>

		<property name="sort" type="java.lang.Integer">
			<column name="SORT" length="8" not-null="true">
				<comment>排序</comment>
			</column>
		</property>

		<many-to-one name="pri" class="com.riversoft.platform.po.CmPri" cascade="all" lazy="false">
			<column name="PRI" length="100" not-null="true">
				<comment>展示权限</comment>
			</column>
		</many-to-one>

	</class>
</hibernate-mapping>
