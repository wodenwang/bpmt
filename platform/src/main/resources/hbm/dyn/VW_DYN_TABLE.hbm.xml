<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class entity-name="VwDynTable" table="VW_DYN_TABLE">
    <cache usage="read-write" />
    <comment>动态表视图模块</comment>

    <id name="viewKey" type="java.lang.String">
      <column name="VIEW_KEY" length="100" not-null="true">
        <comment>主键KEY</comment>
      </column>
    </id>

    <property name="name" type="java.lang.String">
      <column name="NAME" length="100" not-null="true">
        <comment>表名</comment>
      </column>
    </property>

    <property name="logTable" type="java.lang.String">
      <column name="LOG_TABLE" length="100" not-null="false">
        <comment>操作日志表</comment>
      </column>
    </property>

    <property name="busiName" type="java.lang.String">
      <column name="BUSI_NAME" length="200" not-null="true">
        <comment>展示名</comment>
      </column>
    </property>

    <property name="sortName" type="java.lang.String">
      <column name="SORT_NAME" length="100" not-null="true">
        <comment>排序字段</comment>
      </column>
    </property>

    <property name="dir" type="java.lang.String">
      <column name="DIR" length="100" not-null="true">
        <comment>排序方向</comment>
      </column>
    </property>

    <property name="col" type="java.lang.Integer">
      <column name="COL" length="8" not-null="true">
        <comment>展示分列数量</comment>
      </column>
    </property>

    <property name="pageLimit" type="java.lang.Integer">
      <column name="PAGE_LIMIT" length="8" not-null="false">
        <comment>每页数量</comment>
      </column>
    </property>

    <!-- 1:初始化查询;0:被动查询 -->
    <property name="initQuery" type="java.lang.Integer">
      <column name="INIT_QUERY" length="8" not-null="true" default="1">
        <comment>初始化查询</comment>
      </column>
    </property>

    <!--js脚本 -->
    <property name="listJsType" type="java.lang.Integer">
      <column name="LIST_JS_TYPE" length="8">
        <comment>客户端脚本类型</comment>
      </column>
    </property>
    <property name="listJsScript" type="text">
      <column name="LIST_JS_SCRIPT">
        <comment>客户端脚本</comment>
      </column>
    </property>

    <property name="formJsType" type="java.lang.Integer">
      <column name="FORM_JS_TYPE" length="8">
        <comment>客户端脚本类型</comment>
      </column>
    </property>
    <property name="formJsScript" type="text">
      <column name="FORM_JS_SCRIPT">
        <comment>客户端脚本</comment>
      </column>
    </property>

    <!-- 处理器 -->
    <set name="beforeExecs" inverse="true" cascade="delete" lazy="false" order-by="sort asc">
      <cache usage="read-write" />
      <comment>前置处理器列表</comment>
      <key>
        <column name="VIEW_KEY" />
      </key>
      <one-to-many entity-name="VwDynExecBefore" />
    </set>
    <set name="afterExecs" inverse="true" cascade="delete" lazy="false" order-by="sort asc">
      <cache usage="read-write" />
      <comment>前置处理器列表</comment>
      <key>
        <column name="VIEW_KEY" />
      </key>
      <one-to-many entity-name="VwDynExecAfter" />
    </set>

    <set name="prepareExecs" inverse="true" cascade="delete" lazy="false" order-by="sort asc">
      <cache usage="read-write" />
      <comment>展示变量列表</comment>
      <key>
        <column name="VIEW_KEY" />
      </key>
      <one-to-many entity-name="VwDynExecPrepare" />
    </set>

    <set name="parents" inverse="true" cascade="delete" lazy="false" order-by="sort asc">
      <cache usage="read-write" />
      <comment>主表变量列表</comment>
      <key>
        <column name="VIEW_KEY" />
      </key>
      <one-to-many entity-name="VwDynParent" />
    </set>

    <set name="columns" inverse="true" cascade="delete" lazy="false" order-by="sort asc">
      <cache usage="read-write" />
      <comment>字段列表</comment>
      <key>
        <column name="VIEW_KEY" />
      </key>
      <one-to-many entity-name="VwDynColumn" not-found="exception" />
    </set>

    <set name="showColumns" inverse="true" cascade="delete" lazy="false" order-by="sort asc">
      <cache usage="read-write" />
      <comment>展示类字段列表</comment>
      <key>
        <column name="VIEW_KEY" />
      </key>
      <one-to-many entity-name="VwDynColumnShow" />
    </set>

    <set name="formColumns" inverse="true" cascade="delete" lazy="false" order-by="sort asc">
      <cache usage="read-write" />
      <comment>表单类字段列表</comment>
      <key>
        <column name="VIEW_KEY" />
      </key>
      <one-to-many entity-name="VwDynColumnForm" />
    </set>

    <set name="lineColumns" inverse="true" cascade="delete" lazy="false" order-by="sort asc">
      <cache usage="read-write" />
      <comment>分割线字段列表</comment>
      <key>
        <column name="VIEW_KEY" />
      </key>
      <one-to-many entity-name="VwDynColumnLine" />
    </set>

    <set name="querys" inverse="true" cascade="delete" lazy="false" order-by="sort asc">
      <cache usage="read-write" />
      <comment>查询条件列表</comment>
      <key>
        <column name="VIEW_KEY" />
      </key>
      <one-to-many entity-name="VwDynQuery" />
    </set>

    <set name="extQuerys" inverse="true" cascade="delete" lazy="false" order-by="sort asc">
      <cache usage="read-write" />
      <comment>查询条件列表(高级)</comment>
      <key>
        <column name="VIEW_KEY" />
      </key>
      <one-to-many entity-name="VwDynQueryExt" />
    </set>

    <set name="sysSubs" inverse="true" cascade="delete" lazy="false" order-by="sort asc">
      <cache usage="read-write" />
      <comment>系统子标签</comment>
      <key>
        <column name="VIEW_KEY" />
      </key>
      <one-to-many entity-name="VwDynSubSys" />
    </set>

    <set name="viewSubs" inverse="true" cascade="delete" lazy="false" order-by="sort asc">
      <cache usage="read-write" />
      <comment>视图子表标签</comment>
      <key>
        <column name="VIEW_KEY" />
      </key>
      <one-to-many entity-name="VwDynSubView" />
    </set>

    <set name="limits" inverse="true" cascade="delete" lazy="false" order-by="sort asc">
      <cache usage="read-write" />
      <comment>数据限制列表</comment>
      <key>
        <column name="VIEW_KEY" />
      </key>
      <one-to-many entity-name="VwDynLimit" />
    </set>

    <set name="sysBtns" inverse="true" cascade="delete" lazy="false" order-by="sort asc">
      <cache usage="read-write" />
      <comment>系统内置按钮</comment>
      <key>
        <column name="VIEW_KEY" />
      </key>
      <one-to-many entity-name="VwDynBtnSys" />
    </set>

    <set name="itemBtns" inverse="true" cascade="delete" lazy="false" order-by="sort asc">
      <cache usage="read-write" />
      <comment>明细按钮</comment>
      <key>
        <column name="VIEW_KEY" />
      </key>
      <one-to-many entity-name="VwDynBtnItem" />
    </set>

    <set name="summaryBtns" inverse="true" cascade="delete" lazy="false" order-by="sort asc">
      <cache usage="read-write" />
      <comment>汇总按钮</comment>
      <key>
        <column name="VIEW_KEY" />
      </key>
      <one-to-many entity-name="VwDynBtnSummary" />
    </set>

    <!-- 微信设置 -->
    <one-to-one name="weixin" entity-name="VwDynWeixin" cascade="delete" lazy="false">
    </one-to-one>

    <property name="createDate" type="java.util.Date">
      <column name="CREATE_DATE" length="23" not-null="true">
        <comment>创建时间</comment>
      </column>
    </property>

    <property name="updateDate" type="java.util.Date">
      <column name="UPDATE_DATE" length="23" not-null="true">
        <comment>更新时间</comment>
      </column>
    </property>

    <property name="description" type="java.lang.String">
      <column name="DESCRIPTION" length="500">
        <comment>描述</comment>
      </column>
    </property>

  </class>
</hibernate-mapping>
