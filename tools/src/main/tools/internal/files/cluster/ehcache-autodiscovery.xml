<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="false" monitoring="autodetect" dynamicConfig="true" name="bpmtHibernateCache">

    <diskStore path="java.io.tmpdir/ehcache/bpmt/hibernate" />

    <cacheManagerPeerProviderFactory class="net.sf.ehcache.distribution.RMICacheManagerPeerProviderFactory" properties="peerDiscovery=automatic,multicastGroupAddress=230.0.0.1,multicastGroupPort=4446" />
    
	<cacheManagerPeerListenerFactory class="net.sf.ehcache.distribution.RMICacheManagerPeerListenerFactory" />

    <defaultCache maxEntriesLocalHeap="50000" eternal="false" timeToIdleSeconds="1800" timeToLiveSeconds="3600" overflowToDisk="true" maxEntriesLocalDisk="100000" />

</ehcache>